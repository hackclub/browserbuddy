{
    "name": "Ramadan Timer",
    "description": "A simple Ramadan Timer for muslims to find time of suhoor and iftar.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "alarms",
        "notifications",
        "storage",
        "geolocation"
    ],
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}