{
    "manifest_version": 3,
    "name": "To-Do List",
    "version": "1.0",
    "description": "A simple to-do list extension. Created by Adishree Das.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/con48.png",
        "128": "icons/con128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  
