{
    "name": "Call of Prayer",
    "description": "A simple prayer time finder for muslims to find time of prayer.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "alarms",
        "notifications",
        "storage",
        "geolocation"
    ],
    "action":{
        "default_popup": "popup.html",
        "default_icon": {
            "16": "muslim.png",
            "48": "muslim (1).png",
            "128": "muslim (2).png"
        }

    }
}