{
  "manifest_version": 3,
  "name": "Listen Tracker",
  "version": "1.0",
  "description": "Keep track of what you have listened to.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
}
