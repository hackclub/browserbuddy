{
  "manifest_version": 3,
  "name": "Places to Visit Wishlist",
  "version": "1.0",
  "description": "Save and manage a wishlist of places you want to visit.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
