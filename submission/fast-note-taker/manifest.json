{
  "manifest_version": 3,
  "name": "Note Taking Extension",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["dashboard.html"],
    "matches": ["<all_urls>"]
  }],
  "browser_specific_settings": {
    "gecko": {
      "id": "your_extension_id@example.com"  // Optional: provide a unique ID for Firefox
    }
  }
}
